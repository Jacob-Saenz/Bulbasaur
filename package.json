  "main": "server/index.js",
  "scripts": {
    "start": "npm run build && ng serve -o",
    "serve": "node --max-old-space-size=512 dist",
    "server": "tsc && npm run serve",
    "build": "ng build --aot --prod --build-optimizer && tsc",
    "heroku-postbuild": "npm run build",
    "test": "ng lint && tslint --project tslint.json 'server/**/*.{ts,tsx}' && tslint --project tslint.json 'index.ts'",
    "watch": "nodemon -e ts -w ./server -w index.ts -x npm run server"
  },
  "private": true,
