    "eslint": "^6.8.0",
    "husky": "^4.2.3",
    "nodemon": "^2.0.2",
    "protractor": "^5.4.3",
    "ts-node": "~3.2.0",
    "tslint": "~5.7.0",
    "typescript": "~3.7.5"
